{
  "name": "@tracespace/renderer",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0-unreleased",
  "description": "Render @tracespace/plotter image trees as SVGs",
  "types": "./lib/index.d.ts",
  "exports": {
    "types": "./lib/index.d.ts",
    "source": "./src/index.ts",
    "import": "./dist/tracespace-renderer.js",
